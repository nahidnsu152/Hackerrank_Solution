    "# Complete the staircase function below.\n",
    "def staircase(n):\n",
    "    for i in range(n):\n",
    "        a = i+1\n",
    "        print(\" \"*(n-a) + \"#\"*a)\n",
    "if __name__ == '__main__':\n",
    "    n = int(input())\n",
    "\n",
    "    staircase(n)\n"
